<launch>
	<!-- Disable if running online and not from a bag -->
	<param name="use_sim_time" value="false"/>

	<!-- Arguments -->
  	<arg name="map_file" default="$(find robotics_lab2)/maps/IST_map_disc.yaml"/>

	<!-- Map server -->
 	<node pkg="map_server" name="map_server" type="map_server" args="$(arg map_file)"/>

 	<!-- rviz -->
	<node pkg="rviz" type="rviz" name="rviz" args="-d $(find robotics_lab2)/rviz/test_car_sized_cells.rviz"/>

	<!-- grid_to_states -->
  	<!--<node name="mdp" pkg="robotics_lab2" type="mdp_new.py" output="screen"/>-->

</launch>