<launch>
	<!-- Disable if running online and not from a bag -->
	<param name="use_sim_time" value="false"/>

	<!-- Arguments -->
  	<arg name="map_file" default="$(find my_amcl)/maps/disc_map_sim.yaml"/>

	<!-- Map server -->
 	<node pkg="map_server" name="map_server" type="map_server" args="$(arg map_file)"/>

 	<!-- rviz -->
	<!--<node pkg="rviz" type="rviz" name="rviz" args="-d $(find my_amcl)/rviz/test_g_t_c.rviz"/>-->

	<!-- grid_to_states -->
  	<node name="mdp" pkg="my_amcl" type="mdp_new.py" output="screen"/>

</launch>